{  
"wlr/taskbar": {
    "format": "{icon}",
    "icon-size": 20,
    "icon-theme": "Numix-Circle",
    "tooltip-format": "{title}",
    "on-click": "activate",
    "on-click-middle": "close",
    "ignore-list": [
       "Alacritty"
    ],
    "app_ids-mapping": {
      "firefoxdeveloperedition": "firefox-developer-edition"
    },
    "rewrite": {
        "Firefox Web Browser": "Firefox",
        "Foot Server": "Terminal"
    }
},
"hyprland/workspaces": {
	"format": "{icon}{windows}",
	"format-icons": {
        "1": "Û∞é§",
        "2": "Û∞éß",
        "3": "Û∞é™",
        "4": "Û∞é≠",
        "5": "Û∞é±",
        "6": "Û∞é≥",
        "7": "Û∞é∂",
        "8": "Û∞éπ",
        "9": "Û∞éº",
        "10": "Û∞ΩΩ",
	},
},

  "tray": {
    // "icon-size": 21,
    "spacing": 10,
  },
  "clock": {
    "timezone": "Europe/Warsaw",
    "tooltip-format": "{:%Y %B}\n{calendar}",
    "format-alt": "{:%Y-%m-%d}",
  },
  "cpu": {
    "format": "{usage}% Ôãõ",
    "tooltip": false,
  },
  "memory": {
    "format": "{}% RAM",
  },
"wireplumber": {
	"format": "{volume}%",
	"format-muted": "ÔÄ¶  ",
	"on-click": "wpctl set-mute @DEFAULT_SINK@ toggle",
},
"mpris": {
	"format": " {player_icon} ",
	"format-paused": " {status_icon} ",
    "tooltip-format": "",
	"player-icons": {
		"default": "ÔÅå",
		"mpv": "üéµ"
	},
	"status-icons": {
		"paused": "ÔÅã"
	},
},
"custom/artist": {
    "align" : 0,
    "format": "Author: {}",
    "on-click": "playerctl play-pause",
    "exec": "while [ 1 ]; do playerctl metadata xesam:artist && sleep 0.01; done",
    // "interval": 5,
},
"custom/title": {
    "align": 0,
    "format": "{}",
    "on-click": "playerctl play-pause",
    "exec": "while [ 1 ]; do playerctl metadata xesam:title && sleep 0.01; done",
    // "interval": "once",
},
"group/media#control":{
    "orientation": "horizontal",
    "modules": [
        "mpris",
        "group/media#player",
    ],
},
"group/media#player": {
    "orientation": "vertical",
    "modules": [
        "custom/title",
        "custom/artist"
    ]
},
"group/volume": {
    "orientation": "horizontal",
    "modules": [
        "wireplumber",
        "pulseaudio/slider",
    ],
}
}
